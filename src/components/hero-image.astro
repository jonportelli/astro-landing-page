---
import { Image } from "astro:assets";
import moonImage from "https://res.cloudinary.com/diex8siap/image/upload/v1712157055/upscaled_image_ydzhuh.png";

const widths = [450, 800, 1200];
const sizes = "100vw";
---

<Image
  class="h-full w-full object-cover"
  src={moonImage}
  widths={widths}
  sizes={sizes}
  alt="The ridged surface of the moon"
/>
